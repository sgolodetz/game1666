<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.gxstudios.net/game1666/GameConfig.xsd" elementFormDefault="qualified" xmlns="http://www.gxstudios.net/game1666/GameConfig.xsd" xmlns:mstns="http://www.gxstudios.net/game1666/GameConfig.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="BlueprintType">
    <xs:sequence>
      <xs:element name="property" type="PropertyType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="name" type="xs:string"/>
    <xs:attribute name="type" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="PropertyType">
    <xs:attribute name="name" type="xs:string"/>
    <xs:attribute name="type" type="xs:string"/>
  </xs:complexType>

  <xs:element name="config">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="blueprints">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="building" type="BlueprintType" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="walker" type="BlueprintType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
